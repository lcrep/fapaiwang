<view><block wx:if="{{result!=null}}"><view class="recordDetailPage"><view class="recordPersonBox"><view class="recordPersonInfo"><image class="recordPersonPic" mode="aspectFill" src="{{result.headImgUrl}}"></image><view class="recordPersonInfoCont"><view class="recordPersonName">{{result.createEmployeeName+"的客户"}}</view><view class="recordTime">{{''+result.createTime+''}}</view></view></view><view data-event-opts="{{[['tap',[['gotoProcess',['$0'],['result.visitId']]]]]}}" class="nowStatus" bindtap="__e">当前进展</view></view><view class="recordDetailCont"><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">客户姓名:</view><view data-event-opts="{{[['tap',[['gotoSelfPage',['$0'],['result.customerId']]]]]}}" class="detailHeadLink" bindtap="__e"><text class="detailHeadLinkLabel">查看个人主页</text><uni-icons vue-id="6ad1530d-1" type="arrowright" size="18" color="#B8B8B8" bind:__l="__l"></uni-icons></view></view><view class="recordDetailText">{{result.customerName+"  "+result.customerMobile}}</view></view><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">意向房源:</view><view class="detailHeadLink"><text class="detailHeadLinkLabel">查看房源详情</text><uni-icons vue-id="6ad1530d-2" type="arrowright" size="18" color="#B8B8B8" bind:__l="__l"></uni-icons></view></view><view class="recordDetailText">{{result.houseName}}</view></view><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">沟通自评分:</view></view><uni-rate class="recordDetailRate" vue-id="6ad1530d-3" margin="{{8}}" active-color="#d03b41" is-fill="{{false}}" color="#dadada" disabled="{{true}}" size="{{18}}" value="{{result.evaluateScore}}" bind:__l="__l"></uni-rate></view><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">日志描述:</view></view><view class="recordDetailText">{{result.content}}</view></view></view></view></block><view data-event-opts="{{[['tap',[['gotoAdd',['$event']]]]]}}" class="recordFixBtn" bindtap="__e"><uni-icons class="fixBtnIcon" vue-id="6ad1530d-4" type="compose" color="#ffffff" size="20" bind:__l="__l"></uni-icons><text class="fixBtnText">写日志</text></view></view>