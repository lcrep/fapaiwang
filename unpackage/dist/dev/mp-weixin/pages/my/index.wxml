<view class="container"><block wx:if="{{!hasLogin}}"><view class="userBox"><view class="userInfoBox"><image class="defaultUserPhoto" src="../../static/images/defaultUserPic.png"></image><view class="userInfoBoxCont"><view class="userName nowrap">未登录</view><view class="userInfoBoxBot"><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="userInfoBtn noLogin" bindtap="__e">登录/注册</view></view></view></view></view></block><block wx:else><view class="userBox"><image class="blurBg" src="../../static/images/logo.png" mode="aspectFill"></image><view class="userBoxShadow"></view><view class="userInfoBox"><image class="userPhoto" src="../../static/images/defaultUserPic.png"></image><view class="userInfoBoxCont"><view class="userName nowrap">{{''+userInfo.mobile+''}}</view><view class="userInfoBoxBot"><view data-event-opts="{{[['tap',[['edit',['$event']]]]]}}" class="userInfoBtn" bindtap="__e"><text class="editIcon"></text>编辑资料</view><view data-event-opts="{{[['tap',[['gotoSelfPage',['$event']]]]]}}" class="toSelfPage" bindtap="__e">个人主页<uni-icons class="gotoIcon" vue-id="4a180d5e-1" color="#B8B8B8" size="18" type="arrowright" bind:__l="__l"></uni-icons></view></view></view></view></view></block><view class="userBtnsBox1"><uni-grid vue-id="4a180d5e-2" column="{{4}}" show-border="{{false}}" data-event-opts="{{[['^change',[['btnTap1']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{navBtns1}}" wx:for-item="item" wx:for-index="index" wx:key="index"><uni-grid-item vue-id="{{('4a180d5e-3-'+index)+','+('4a180d5e-2')}}" index="{{index}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="grid-item-box"><image class="navBtnImg" src="{{item.src}}" mode="aspectFill"></image><view class="navBtnName">{{''+item.name+''}}</view></view></uni-grid-item></block></uni-grid></view><block wx:if="{{hasLogin}}"><view class="userBtnsBox2"><uni-grid vue-id="4a180d5e-4" column="{{4}}" show-border="{{false}}" data-event-opts="{{[['^change',[['btnTap2']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{navBtns2}}" wx:for-item="item" wx:for-index="index" wx:key="index"><uni-grid-item vue-id="{{('4a180d5e-5-'+index)+','+('4a180d5e-4')}}" index="{{index}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="grid-item-box"><image class="navBtnImg" src="{{item.src}}" mode="aspectFill"></image><view class="navBtnName">{{''+item.name+''}}</view></view></uni-grid-item></block></uni-grid><button class="logoutBtn" type="default" data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" bindtap="__e">退出登录</button></view></block></view>