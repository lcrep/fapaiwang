
.selectCityBox{
	padding: 0 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	position: fixed;
	width: 100%;
	height: 68rpx;
	top: 0;	
	background-color: #FFFFFF;
	z-index: 999;
	/* top:calc(44px + env(safe-area-inset-top)); */
}
.selCityLabel{
	font-size: 26rpx;
	color: #555555;
}
.cityPicker {
	max-width: 250rpx;
}
.citySel .cityName {
	display: block;
	max-width: 150rpx;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	font-size: 30rpx;
	color: #222222;
	margin-right: 5rpx;
}
.goodsBox{
	padding: 30rpx 30rpx 0 30rpx;
}


