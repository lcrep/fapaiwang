
.fixHead{
	position: fixed;

	top: 0;




	width: 100%;
	height: 164rpx;
	background-color: #FFFFFF;
	z-index: 999;
	left: 0;
}
.tableHead{
	width: 100%;
	height: 76rpx;
	background-color: #444444;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 20rpx;
}
.tableTd{
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 0 10rpx;
	text-overflow: -o-ellipsis-lastline;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	line-clamp: 2;
	-webkit-box-orient: vertical;
	font-size: 30rpx;
	color: #222222;
	width: 50%;
	text-align: center;
}
.tableHead .tableTd{
	color: #FFFFFF;
	font-size: 26rpx;
	line-height: 76rpx;
}
.selectCityBox {
	padding: 0 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width: 100%;
	height: 68rpx;
}
.selCityLabel {
	font-size: 26rpx;
	color: #555555;
}
.cityPicker {
	max-width: 250rpx;
}
.citySel .cityName {
	display: block;
	max-width: 150rpx;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	font-size: 30rpx;
	color: #222222;
	margin-right: 5rpx;
}
.content {
	padding-top: 164rpx;
}
.areaList{
}
.areaItem{
	position: relative;	
	width: 100%;
	height: 100rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.areaItem::after{
	position: absolute;
	content: "";
	left: 30rpx;
	right: 30rpx;
	bottom: 0;
	background-color: #F7F7F7;
	height: 1rpx;
}

