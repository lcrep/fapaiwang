/* 公共css */
page {
	font-size: 28rpx;
}
.nowrap {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.title-contents {
	height: calc(25px + 108rpx);
}
.status {
	height: 25px;
	background: #FFFFFF;
}
.top-view {
	width: 100%;
	position: fixed;
	top: 0;
	z-index: 9999;
}
.titles {
	width: 100%;
	height: 108rpx;
	position: fixed;
	top: 25px;
	background: #ffffff;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	padding: 20rpx;
	box-sizing: border-box;
	z-index: 9999;
}
.citySel {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin-left: 30rpx;
	height: 68rpx;
}
.citySel .cityName {
	display: block;
	max-width: 150rpx;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	font-size: 30rpx;
	color: #222222;
	margin-right: 5rpx;
}
.downIcon {
	font-family: uniicons;
	font-size: 20px;
	font-weight: normal;
	font-style: normal;
	width: 20px;
	color: #222222;
}
.searchInputBox {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background: #F7F7F7;
	margin-left: 18rpx;
	height: 68rpx;
	padding: 0 16rpx;
	border-radius: 5px;
}
.searchInputBox .uni-input {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	margin-left: 10rpx;
	font-size: 28rpx;
}
/* 房源item */
.goodsItem {
	width: 100%;
	height: 244rpx;
	margin-bottom: 30rpx;
	display: -webkit-box;
}
.goodsPic {
	width: 328rpx;
	height: 244rpx;
	overflow: hidden;
	border-radius: 10rpx;
	position: relative;
}
.goodsPic .goodsImage {
	width: 100%;
	height: 100%;
}
.goodsTag {
	position: absolute;
	left: 0;
	top: 0;
	width: 80rpx;
	height: 36rpx;
	text-align: center;
	line-height: 36rpx;
	display: block;
	background: -webkit-linear-gradient(334deg, #FFEEA0 0%, #FFD561 90%);
	background: linear-gradient(116deg, #FFEEA0 0%, #FFD561 90%);
	color: #222222;
	font-size: 24rpx;
	z-index: 1;
}
.goodsInfo {
	margin-left: 20rpx;
	width: 342rpx;
	height: 244rpx;
}
.goodsName {
	font-size: 30rpx;
	color: #555555;
	line-height: 42rpx;
	text-overflow: -o-ellipsis-lastline;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	line-clamp: 2;
	-webkit-box-orient: vertical;
}
.goodsAddress {
	width: 100%;
	font-size: 24rpx;
	color: #b8b8b8;
	line-height: 34rpx;
	margin-top: 10rpx;
}
.goodsPrice {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: end;
	-webkit-align-items: flex-end;
	        align-items: flex-end;
	margin-top: 20rpx;
}
.priceLabel {
	display: inline-block;
	font-size: 24rpx;
	color: #555555;
	vertical-align: bottom;
}
.priceNum {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	display: inline-block;
	color: #CD282F;
	font-size: 36rpx;
	margin-left: 10rpx;
	line-height: 36rpx;
}
.goodsPrice .discount {
	color: #CD282F;
	font-size: 26rpx;
	float: right;
	display: block;
}
.goodsStatus {
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	border-radius: 8rpx;
	overflow: hidden;
	margin-top: 20rpx;
}
.statusName {
	display: block;
	width: 92rpx;
	height: 100%;
	text-align: center;
	background: #CD282F;
	color: #ffffff;
	font-size: 24rpx;
}
.goodsTime {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	padding-left: 20rpx;
	background-color: #F7F7F7;
	color: #555555;
	font-size: 20rpx;
}
.listTabs {
	height: 56rpx;
	padding: 10rpx 0 30rpx 0;
}
.tabItem {
	float: left;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	line-height: 52rpx;
	border: 1rpx solid #B8B8B8;
	padding: 0 20rpx;
	margin-right: 20rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	color: #B8B8B8;
	border-radius: 6rpx;
}
.tabActive {
	color: #CD282F !important;
	border: 1rpx solid #CD282F !important;
}
.sortIcon {
	width: 30rpx;
	height: 30rpx;
	margin-left: 10rpx;
	display: inline-block;
	background: url(data:image/png;base64,);
	background-size: cover;
}
.sortIconUp {
	background: url(data:image/png;base64,) !important;
	background-size: cover !important;
}
