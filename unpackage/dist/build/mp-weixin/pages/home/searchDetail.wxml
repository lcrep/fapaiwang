<view class="container"><view class="content uni-padding-wrap uni-common-mt"><view class="{{[(true)?'weixinHead':'',(scrollTop>1)?'weixinHeadFixed':'']}}"><view class="searchInputBox"><uni-icons class="searchIcon" vue-id="5da53166-1" color="#f44a33" size="18" type="search" bind:__l="__l"></uni-icons><input class="uni-input" placeholder="搜索标的物名称" confirm-type="search" placeholder-style="font-size:26rpx;color:#B8B8B8;" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['__set_model',['','searchVal','$event',[]]]]]]}}" value="{{searchVal}}" bindconfirm="__e" bindinput="__e"/></view><picker mode="multiSelector" value="{{cityIndex}}" range="{{cities}}" range-key="name" data-event-opts="{{[['columnchange',[['cityChange',['$event']]]],['change',[['citySel',['$event']]]]]}}" bindcolumnchange="__e" bindchange="__e"><view class="citySel"><text class="cityName">{{theCity}}</text><uni-icons vue-id="5da53166-2" type="arrowdown" size="20" bind:__l="__l"></uni-icons></view></picker></view><block wx:if="{{scrollTop>1}}"><view class="weixinHead"></view></block><view class="listTabs"><view data-event-opts="{{[['tap',[['selectTab',[0]]]]]}}" class="{{[(true)?'tabItem':'',(tabCurrent==0)?'tabActive':'']}}" bindtap="__e"><text>最新发布</text></view><view data-event-opts="{{[['tap',[['selectTab',[1]]]]]}}" class="{{[(true)?'tabItem':'',(tabCurrent==1)?'tabActive':'']}}" bindtap="__e"><text>收藏更多</text></view><view data-event-opts="{{[['tap',[['selectTab',[2]]]]]}}" class="{{[(true)?'tabItem':'',(tabCurrent==2)?'tabActive':'']}}" bindtap="__e"><text>价格</text><text class="{{[(true)?'sortIcon':'',(priceSort==1)?'sortIconUp':'']}}"></text></view></view><view class="goodsBox"><block wx:if="{{total!==0}}"><view class="goodsList"><block wx:for="{{houseList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['itemTap',['$0','$1'],[[['houseList','',index,'paimaiId']],[['houseList','',index,'houseSource']]]]]]]}}" class="goodsItem" bindtap="__e"><view class="goodsPic"><text class="goodsTag">{{item.paimaiTimesText}}</text><image class="goodsImage" src="{{item.productImage}}" mode="aspectFill"></image></view><view class="goodsInfo"><view class="goodsName">{{item.title}}</view><block wx:if="{{item.paimaiStatus==1}}"><view class="goodsAddress nowrap">{{item.accessNum+"次围观/"+item.title}}</view></block><block wx:else><view class="goodsAddress nowrap">{{item.bidCount+"次出价 / "+item.countyName}}</view></block><view class="goodsPrice"><text class="priceLabel">当前价</text><text class="priceNum">{{item.currentPriceText+"万"}}</text><block wx:if="{{item.discount!=10}}"><text class="discount">{{item.discount+"折"}}</text></block></view><block wx:if="{{item.paimaiStatus==1}}"><view class="goodsStatus goodsStatus1"><text class="statusName">未开始</text><text class="goodsTime">{{item.timeText+"开始"}}</text></view></block><block wx:else><block wx:if="{{item.paimaiStatus==2}}"><view class="goodsStatus goodsStatus2"><text class="statusName">拍卖中</text><text class="goodsTime">{{item.timeText+"开始"}}</text></view></block><block wx:else><view class="goodsStatus goodsStatus3"><text class="statusName">已结束</text><text class="goodsTime">{{item.timeText+"结束"}}</text></view></block></block></view></view></block><uni-load-more vue-id="5da53166-3" iconType="snow" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></view></block><block wx:else><view class="noData"><image class="noDataImg" src="../../static/images/noRecord.png" mode></image><text class="noDataText">暂无数据</text></view></block></view></view></view>