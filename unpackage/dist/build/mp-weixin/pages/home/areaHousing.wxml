<view><view class="selectCityBox"><text class="selCityLabel">当前城市：</text><picker mode="multiSelector" value="{{cityIndex}}" range="{{cities}}" range-key="name" data-event-opts="{{[['columnchange',[['cityChange',['$event']]]],['change',[['citySel',['$event']]]]]}}" bindcolumnchange="__e" bindchange="__e"><view class="citySel"><text class="cityName">{{theCity}}</text><uni-icons vue-id="17c54b66-1" type="arrowdown" size="20" bind:__l="__l"></uni-icons></view></picker></view><view class="content"><view class="areaSelBox"><view class="selBoxHead"><view class="td td1">区域</view><view class="td td2">法拍行情</view><view class="td td3">月成交量</view></view><view class="selBoxList"><view class="tr selectedItem"><view class="td td1">{{''+selectedArea.countyName+''}}</view><view class="td td2">{{''+selectedArea.avgPrice+'元/m²'}}</view><view class="td td3">{{''+selectedArea.count+'套'}}</view></view><view data-event-opts="{{[['tap',[['openAreaList',['$event']]]]]}}" class="selectBtnBox" bindtap="__e"><uni-icons class="loopIcon" vue-id="17c54b66-2" color="#CD282F" size="16" type="loop" bind:__l="__l"></uni-icons><text class="btnLabel">切换区域</text></view></view></view><view class="goodsBox"><view class="listTabs"><view data-event-opts="{{[['tap',[['selectTab',[0]]]]]}}" class="{{[(true)?'tabItem':'',(tabCurrent==0)?'tabActive':'']}}" bindtap="__e"><text>最新发布</text></view><view data-event-opts="{{[['tap',[['selectTab',[1]]]]]}}" class="{{[(true)?'tabItem':'',(tabCurrent==1)?'tabActive':'']}}" bindtap="__e"><text>收藏更多</text></view><view data-event-opts="{{[['tap',[['selectTab',[2]]]]]}}" class="{{[(true)?'tabItem':'',(tabCurrent==2)?'tabActive':'']}}" bindtap="__e"><text>价格</text><text class="{{[(true)?'sortIcon':'',(priceSort==1)?'sortIconUp':'']}}"></text></view></view><view class="goodsList"><block wx:for="{{houseList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['itemTap',['$0','$1'],[[['houseList','',index,'paimaiId']],[['houseList','',index,'houseSource']]]]]]]}}" class="goodsItem" bindtap="__e"><view class="goodsPic"><text class="goodsTag">{{item.paimaiTimesText}}</text><image class="goodsImage" src="{{item.productImage}}" mode="aspectFill"></image></view><view class="goodsInfo"><view class="goodsName">{{item.title}}</view><block wx:if="{{item.paimaiStatus==1}}"><view class="goodsAddress nowrap">{{"次围观/"+item.title}}</view></block><block wx:else><view class="goodsAddress nowrap">{{item.bidCount+"次出价 / 江夏区"}}</view></block><view class="goodsPrice"><text class="priceLabel">当前价</text><text class="priceNum">{{item.currentPriceText+"万"}}</text><block wx:if="{{item.discount!=10}}"><text class="discount">{{item.discount+"折"}}</text></block></view><block wx:if="{{item.paimaiStatus==1}}"><view class="goodsStatus goodsStatus1"><text class="statusName">未开始</text><text class="goodsTime">{{item.timeText+"开始"}}</text></view></block><block wx:else><block wx:if="{{item.paimaiStatus==2}}"><view class="goodsStatus goodsStatus2"><text class="statusName">拍卖中</text><text class="goodsTime">{{item.timeText+"开始"}}</text></view></block><block wx:else><view class="goodsStatus goodsStatus3"><text class="statusName">已结束</text><text class="goodsTime">{{item.timeText+"结束"}}</text></view></block></block></view></view></block></view></view><uni-load-more vue-id="17c54b66-3" iconType="snow" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></view><block wx:if="{{openAreaSel}}"><view class="areaListBox"><view data-event-opts="{{[['tap',[['openAreaList',['$event']]]]]}}" class="listShardow" bindtap="__e"></view><view class="areaSelBox theOpenList"><view class="selBoxHead"><view class="td td1">区域</view><view class="td td2">法拍行情</view><view class="td td3">月成交量</view></view><view class="selBoxList"><block wx:for="{{areaList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectArea',[index]]]]]}}" class="{{[(true)?'tr':'',(index==selectedArea.index)?'selectedItem':'']}}" bindtap="__e"><view class="td td1">{{''+item.countyName+''}}</view><view class="td td2">{{''+item.avgPrice+'元/m²'}}</view><view class="td td3">{{''+item.count+'套'}}</view></view></block></view></view></view></block></view>