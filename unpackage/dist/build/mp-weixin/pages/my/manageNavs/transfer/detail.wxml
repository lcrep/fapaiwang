<view><block wx:if="{{result!=null}}"><view class="recordDetailPage"><view class="recordDetailCont"><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">客户姓名:</view><view data-event-opts="{{[['tap',[['gotoSelfPage',['$0'],['result.customerId']]]]]}}" class="detailHeadLink" bindtap="__e"><text class="detailHeadLinkLabel">查看个人主页</text><uni-icons vue-id="3b201671-1" type="arrowright" size="18" color="#B8B8B8" bind:__l="__l"></uni-icons></view></view><view class="recordDetailText">{{result.username+" "+result.mobile}}</view></view><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">意向房源:</view><view data-event-opts="{{[['tap',[['gotoHouse',['$0','$1'],['result.paimaiId','result.houseSource']]]]]}}" class="detailHeadLink" bindtap="__e"><text class="detailHeadLinkLabel">查看房源详情</text><uni-icons vue-id="3b201671-2" type="arrowright" size="18" color="#B8B8B8" bind:__l="__l"></uni-icons></view></view><view class="recordDetailText">{{result.houseName}}</view></view><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">业务员:</view></view><view class="recordDetailText">{{result.employeeName}}</view></view><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">办理过户状态:</view></view><block wx:if="{{result.completeState==0}}"><view class="recordDetailText">未完成</view></block><block wx:else><view class="recordDetailText">已完成</view></block></view><block wx:if="{{result.completeState==1}}"><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">办理过户完成时间:</view></view><view class="recordDetailText">{{result.completeTime}}</view></view></block><view class="recordDetailItem"><view class="detailHead"><view class="detailHeadTitle">备注:</view></view><view class="recordDetailText">{{result.memo}}</view></view></view></view></block></view>